{
  "name": "eslint-config-typed-fp",
  "version": "1.7.0",
  "description": "An opinionated ESLint config to encourage pure(ish), typeful functional programming in TypeScript.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/eslint-config-typed-fp.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-functional": "^3.7.0",
    "eslint-plugin-total-functions": "^4.10.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typed-fp": "^1.7.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-total-functions": "^5.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "type-coverage": "^2.19.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 100,
    "strict": true,
    "detail": true
  }
}
